{
  "name": "v10-managed-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "clear": "rimraf __screenshots__",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --setupFiles ./setupFile.js",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 9009",
    "build-storybook": "storybook build --test",
    "storycapture:all": "storycapture --verbose --serverTimeout 100000 --serverCmd \"storybook dev -p 9009\" \"http://localhost:9009\""
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@readyfor/storycapture": "file:../../packages/storycapture",
    "@storybook/addon-links": "^10.0.0",
    "@storybook/cli": "^10.0.0",
    "@storybook/preset-create-react-app": "^10.0.0",
    "@storybook/react": "^10.0.0",
    "@storybook/react-webpack5": "^10.0.0",
    "@testing-library/react": "^14.0.0",
    "rimraf": "^2.6.3",
    "storybook": "^10.0.0"
  }
}
